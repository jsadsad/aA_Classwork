<h1>Sign In</h1>

<form action=<%= session_url%> method="POST">
  <label>Username
    <input type="text" name="user[user_name]" value="username">
  </label>

  <label>Password
    <input type="password" name="user[password]" value="password">
  </label>

  <input type="submit" value="Sign In">
</form>
